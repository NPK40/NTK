<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - pesok.pro</title>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://api-maps.yandex.ru/2.1/?apikey=API-ключ&lang=ru_RU" type="text/javascript"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/styles.css" asp-append-version="true" />

    <link rel="stylesheet" href="~/pesok.pro.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        @await Html.PartialAsync("HeaderTwo")
    </header>
    <div class="container mx-auto mt-10">
         <div style="font-family: OpenSansRegular;color:#434559;" class="text-base"><a style="color:#fc5203;" href="/">Главная</a><span style="color:#fc5203;"> - </span> @ViewData["Header"] </div>
    </div>
    <div class="container mx-auto">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer>
        @await Html.PartialAsync("Footer")
    </footer>
   
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
